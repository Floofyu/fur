local bypassing = false

coroutine.resume(coroutine.create(function()
game:GetService"RunService".RenderStepped:Connect(function()
if _G.Toggle == true then
if _G.Tool ~= nil then
if bypassing == false then
if game.Players.LocalPlayer.Stamina.Value >= 50 then
if not game.Players.LocalPlayer.Character:FindFirstChild(_G.Tool) then game.Players.LocalPlayer.Backpack:FindFirstChild(_G.Tool).Parent = game.Players.LocalPlayer.Character  end
if game.Players.LocalPlayer.Character:FindFirstChild(_G.Tool) then
game.Players.LocalPlayer.Character[_G.Tool].Parent = game.Players.LocalPlayer.Backpack
game.Players.LocalPlayer.Backpack[_G.Tool].Parent = game.Players.LocalPlayer.Character
end 
end
end
end
end
end)
end))

coroutine.resume(coroutine.create(function()
while _G.Chicken == true do
wait()
if bypassing == false then
pcall(function()
if not game.Players.LocalPlayer:FindFirstChild("XPBoost") and not game.Players.LocalPlayer.Character:FindFirstChild("Chicken") then
if game:GetService("Players")[game.Players.LocalPlayer.Name].Backpack:FindFirstChild("Chicken") then
game:GetService("Players")[game.Players.LocalPlayer.Name].Backpack["Chicken"].Parent = game.Players.LocalPlayer.Character
game.Players.LocalPlayer.Character:FindFirstChild("Chicken"):Activate()
end
end
end)
end
end
end))

coroutine.resume(coroutine.create(function()
while _G.Mango == true do
wait()
if bypassing == false then
pcall(function()
if not game.Players.LocalPlayer:FindFirstChild("EnergyBoost") and not game.Players.LocalPlayer.Character:FindFirstChild("Mango Shake") then
if game:GetService("Players")[game.Players.LocalPlayer.Name].Backpack:FindFirstChild("Mango Shake") then
game:GetService("Players")[game.Players.LocalPlayer.Name].Backpack["Mango Shake"].Parent = game.Players.LocalPlayer.Character
game.Players.LocalPlayer.Character:FindFirstChild("Mango Shake"):Activate()
end
end
end)
end
end
end))

coroutine.resume(coroutine.create(function()
while wait(1) do
if _G.BoostFPS == true then game:GetService("RunService"):Set3dRenderingEnabled(false)
elseif _G.BoostFPS == false then game:GetService("RunService"):Set3dRenderingEnabled(true)
end
end
end))

coroutine.resume(coroutine.create(function()
while _G.SafeSpot == true do
wait()
if not game.Players.LocalPlayer.Character:FindFirstChild("Bypassed") then
bypassing = true
	game.Players.LocalPlayer.Character.Humanoid:ChangeState(15)
	wait()
	game.Players.LocalPlayer.Character.Humanoid:Destroy()
	wait()
	local plr = game.Players.LocalPlayer
	local char = plr.Character
	char:ClearAllChildren()
	wait(game.Players.RespawnTime + 2.5)
	for i=1, 3 do
	wait(1)
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(100000,100,0)
	end
	wait(0.5)
	local bypassed = Instance.new('BoolValue', game.Players.LocalPlayer.Character)
	bypassed.Name = "Bypassed"
bypassing = false
elseif game.Players.LocalPlayer.Character:FindFirstChild("Bypassed") then
local YVal = math.random(50,1000)
            local Zval = math.random(8000,100000)
            local safepart = workspace:FindFirstChild("SP")
		if safepart == nil and game.Players.LocalPlayer.Character:FindFirstChild("Bypassed") then
                safepart = Instance.new("Part")
                safepart.Name = 'SP'
                safepart.Parent = workspace
                safepart.Position = Vector3.new(500, YVal, Zval)
                safepart.Anchored = true
                safepart.CanCollide = true
            safepart.Size = Vector3.new(100, 1,100)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = safepart.CFrame + Vector3.new(0,5,0)
		elseif safepart and game.Players.LocalPlayer.Character:FindFirstChild("Bypassed") then
		wait(10)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = safepart.CFrame + Vector3.new(0,5,0)
	  end
end
end
end))

coroutine.resume(coroutine.create(function()
if _G.Vest == true then
wait()
if bypassing == false then
pcall(function()
if not game.Players.LocalPlayer.Character:FindFirstChild("VestWeight") and game.Players.LocalPlayer.Backpack:FindFirstChild("120KG Vest Weight") then
game.Players.LocalPlayer.Backpack["120KG Vest Weight"].Parent = game.Players.LocalPlayer.Character
game.Players.LocalPlayer.Character["120KG Vest Weight"]:Activate()
game.Players.LocalPlayer.Character["120KG Vest Weight"].Parent = game.Players.LocalPlayer.Backpack
end
end)
end
end
end))
